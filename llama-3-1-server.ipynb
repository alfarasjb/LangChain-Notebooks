   "outputs": [],
   "source": [
    "def chat_ollama(user_prompt: str) -> str: \n",
    "    return {\n",
    "        \"role\": \"user\", \n",
    "        \"content\": user_prompt\n",
    "    }\n",
    "response = ollama.chat(model='llama3.1', messages=[\n",
    "    chat_ollama(\"Why is the sky blue?\")\n",
    "])\n",
    "print(response['message']['content']) "
   ]
  },
